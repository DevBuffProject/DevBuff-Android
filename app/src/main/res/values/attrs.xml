<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AppbarIdeaDetails">
        <attr name="title" format="string"/>
        <attr name="subtitle" format="string"/>
        <attr name="status" format="enum">
            <enum name="waiting_full_team" value="0"/>
        </attr>
        <attr name="imageFromUserId" format="string"/>
    </declare-styleable>

    <declare-styleable name="RoundedFrameLayout">
        <attr name="cornerRadius" format="dimension"/>
        <attr name="cornerRadiusTopLeft" format="dimension" />
        <attr name="cornerRadiusTopRight" format="dimension"/>
        <attr name="cornerRadiusBottomLeft" format="dimension"/>
        <attr name="cornerRadiusBottomRight" format="dimension"/>
    </declare-styleable>

</resources>